{
  "name": "incident-management-frontend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "nodemon --watch 'vite.config.ts' --exec vite",
    "build": "vue-tsc --project tsconfig.build.json && vite build",
    "build:prod": "vue-tsc --project tsconfig.build.json && vite build --mode production",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:install": "playwright install",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "type-check": "vue-tsc --noEmit",
    "analyze:bundle": "vite-bundle-analyzer",
    "build:analyze": "vite build && npx vite-bundle-analyzer"
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.13",
    "axios": "^1.6.2",
    "chart.js": "^4.5.0",
    "pinia": "^2.1.7",
    "vue": "^3.4.0",
    "vue-chartjs": "^5.3.2",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vitest/ui": "^1.1.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.19.2",
    "jsdom": "^23.1.0",
    "nodemon": "^3.1.10",
    "playwright": "^1.40.1",
    "postcss": "^8.5.6",
    "prettier": "^3.1.1",
    "tailwindcss": "^4.1.13",
    "terser": "^5.44.0",
    "typescript": "~5.3.3",
    "vite": "^5.0.10",
    "vitest": "^1.1.0",
    "vue-tsc": "^1.8.25"
  },
  "keywords": [
    "vue",
    "typescript",
    "incident-management"
  ],
  "author": "",
  "license": "ISC",
  "description": "Vue.js frontend for Incident Management System"
}
